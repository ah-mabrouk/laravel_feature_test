{
    "name": "solutionplus/laravel-feature-test",
    "description": "A Laravel package that provides utilities to help test REST API endpoints.",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Mabrouk",
            "email": "a.mabrouk@otas.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": ">=9"
    },    
    "require-dev": {
        "orchestra/testbench": "^10.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Solutionplus\\FeatureTest\\SolutionplusFeatureTestServiceProvider"
            ]
        }
    },    
    "autoload": {
        "psr-4": {
            "Solutionplus\\FeatureTest\\": "src/"
        }
    },    
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }    
}
